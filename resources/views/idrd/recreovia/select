select 
	c.descripcion,
	r.id_usuario,
	u.organizacion,
	u.email
from
	respuestas r,
	preguntas p,
	categoria c,
	usuarios u
where
	r.id_usuario = u.id_usuario
	and r.id_pregunta = p.id_pregunta 
	and c.id_categoria = p.id_categoria
	r.id_usuario not in (select id_usuario from experiencias_usuarios)
group by 
	r.id_usuario


select
	c.descripcion
	r.id_usuario
from
	respuestas r,
	preguntas p,
	categorias c
where
	r.id_pregunta = p.id_pregunta
	and p.id_categoria = c.id_categoria
group by
	r.id_usuario 
